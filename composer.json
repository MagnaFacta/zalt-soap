{
    "name": "magnafacta/zalt-soap",
    "description": "Zend Alternative SOAP Model",
    "type": "library",
    "require": {
        "php": ">=5.6.0",
        "magnafacta/zalt-model": "2.x-dev",
        "laminas/laminas-soap": "^2.13",
        "laminas/laminas-servicemanager": "^3.22.0"
    },
    "require-dev": {
        "phpstan/phpstan": "1.*",
        "phpunit/phpunit": "10.*"
    },
    "authors": [
        {
            "name": "Matijs de Jong",
            "email": "mjong@magnafacta.nl"
        },
        {
            "name": "Menno Dekker",
            "email": "menno.dekker@erasmusmc.nl"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Zalt\\Soap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zalt\\Soap\\": "test\\"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/MagnaFacta/zalt-html.git"
        },
        {
            "type": "git",
            "url": "https://github.com/MagnaFacta/zalt-late.git"
        },
        {
            "type": "git",
            "url": "https://github.com/MagnaFacta/zalt-loader.git"
        },
        {
            "type": "git",
            "url": "https://github.com/MagnaFacta/zalt-model.git"
        },
        {
            "type": "git",
            "url": "https://github.com/MagnaFacta/zalt-util.git"
        }
    ],
    "scripts": {
        "test": [
            "@putenv XDEBUG_MODE=coverage",
            "vendor/bin/phpunit --configuration test/phpunit.xml test"
        ],
        "stan": [
            "vendor/bin/phpstan"
        ]
    }
}
